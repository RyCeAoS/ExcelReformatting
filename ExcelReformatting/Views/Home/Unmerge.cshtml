@{
    ViewData["Title"] = "Cell Unmerger";
}


<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/CellUnmerger.css" />
    <title>@ViewData["Title"]</title>
</head>
<body>
    <h1>Unmerge your cells</h1>
    <div class="FileUploadcontainer"> 
        <div class="Info-Box">
            <p>Place an excel file with merged cells that you want unmerged</p> 
        </div>
        <form method="post" asp-controller="Excel" asp-action="FormattedFile" enctype="multipart/form-data">
            <input class="FileInput" type="file" name="file"><br>
            <button class="Submit" type="submit">Upload File  </button>
        </form>
    </div>



</body>
