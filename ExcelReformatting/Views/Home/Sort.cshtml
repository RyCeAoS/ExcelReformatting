@{
    ViewData["Title"] = "Cell Unmerger";
}


<head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="~/css/sort.css" />
    <title>@ViewData["Title"]</title>
</head>
<body>
    <h1>Sort your rows</h1>
    <div class="FileUploadcontainer">
        <div class="Info-Box">
            <p>Place an excel sheet so with rows that you want sorted</p>
        </div>
        <form method="post" asp-controller="Sort" asp-action="SortedFile" enctype="multipart/form-data">
            <label class="label" for="input">Upload File</label>
            <input class="FileInput" type="file" name="file" id="input" multiple><br>
            <button class="Submit" type="submit">Download File </button>
        </form>
    </div>



</body>
